{
  "name": "@planship/svelte",
  "version": "0.3.0-alpha18",
  "author": "Pawel Wojnarowicz",
  "license": "MIT",
  "homepage": "https://planship.io",
  "type": "module",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./coverage",
    "test:types": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint --ext .svelte,.ts ./src",
    "test:lib": "vitest",
    "test:lib:dev": "pnpm run test:lib --watch",
    "test:build": "publint --strict",
    "test:attw": "attw --pack",
    "build": "rm -rf ./dist && svelte-package --input ./src --output ./dist",
    "publish:alpha": "pnpm build && pnpm publish --tag alpha --no-git-checks"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@testing-library/svelte": "^5.1.0",
    "eslint-plugin-svelte": "^2.40.0",
    "svelte": "^4.2.17",
    "svelte-check": "^3.7.1",
    "svelte-eslint-parser": "^0.39.1"
  },
  "peerDependencies": {
    "svelte": "^3.54.0 || ^4.0.0 || ^5.0.0-next.0"
  },
  "dependencies": {
    "@planship/fetch": "0.3.0-alpha6"
  }
}
